GAL system {
         // Variable declarations
         int var = 5;
         array[2] tab = (1, 2);
         list l = (1, 23, -4);


         // Transitions
         transition t1 [ var > 9] {
                 tab[0] = tab[1] * (peek(l)+3);
                 var = var * 5;
         }


         transition t2 [ peek(l) == 23 ] label  "a" {
                 tab[1] = 0;
         }


         TRANSIENT = False;
}
