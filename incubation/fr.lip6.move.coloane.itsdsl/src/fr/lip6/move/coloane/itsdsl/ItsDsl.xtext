grammar fr.lip6.move.coloane.itsdsl.ItsDsl with org.eclipse.xtext.common.Terminals

import "http://coloane.lip6.fr/model/1.0" as model
import "http://www.eclipse.org/emf/2002/Ecore" as ecore

generate itsdsl "http://coloane.lip6.fr/model/ItsDsl"

Model:
	elements+=Element*
;

Element:
	"element" name=ID "mapsTo" ref=[model::Tattribute|FQN]
;

FQN returns ecore::EString:
	ID ("." ID)*
;
